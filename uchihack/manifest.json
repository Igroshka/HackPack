{
    "manifest_version": 2,
    "name": "UchiDone",
    "version": "2.4.5",

    "author": "YUFI",
    "description": "Injects UchiDone v2.4.5",
    "permissions": [ "<all_urls>", "tabs", "activeTab", "storage", "webNavigation" ],

    "background": {
        "page": "injector/background.html"
    },

    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "96": "icons/96.png",
        "192": "icons/192.png"
    }
}
